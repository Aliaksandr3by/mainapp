{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"modules": true,
			"experimentalObjectRestSpread": true,
			"arrowFunctions": true,
			"blockBindings": true,
			"restParams": true,
			"spread": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module",
		"impliedStrict": true
	},
	"plugins": [
		"react"
	],
	"rules": {
		"comma-dangle": 0,
		"react/jsx-uses-vars": 1,
		"react/display-name": 1,
		"no-unexpected-multiline": "warn",
		"no-const-assign": "warn",
		"no-this-before-super": "warn",
		"no-undef": "warn",
		"no-var": "error",
		"eqeqeq": "error",
		"strict": "error",
		"prefer-const": "error",
		"no-unreachable": "warn",
		"no-unused-vars": "warn",
		"constructor-super": "warn",
		"valid-typeof": "warn",
		"no-console": "off",
		"linebreak-style": [
			"error",
			"windows"
		],
		"quotes": [
			"warn",
			"double",
			{
				"allowTemplateLiterals": true
			}
		],
		"semi": [
			"error",
			"always"
		]
	}
}